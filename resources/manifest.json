{
    "name": "Lucia",
    "description": "Chrome-Extension-Framework",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "management",
        "storage",
        "scripting"
    ],
    "action": {
      "default_title": "Lucia",
      "default_popup": "./lucia.html"
    },
    "content_security_policy": {
      "extension_page":"script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
}